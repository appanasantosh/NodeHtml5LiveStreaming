<!DOCTYPE html>
<html>
  <head>
   
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" >
     <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
     <script src="http://jquery-json.googlecode.com/files/jquery.json-2.2.min.js"></script>
     
     <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" ></script>
    <meta charset="utf-8">
    <style type="text/css">
      .box{height: 200px; width :200px; background-color: blue;position: absolute;}
      .box1{height: 200px; width :200px; background-color: blue;position: absolute;}
      .box2{height: 200px; width :200px; background-color: blue;position: absolute;}

    </style>
    <script type="text/javascript">
     $(document).ready(function(){
        $('#prop').click(function(e)
        {
          alert("bubbled");
        });
        $('#test').click(function(e){
          //e.stopPropagation();
          e.preventDefault();
          alert("clicked");
        });

        $("a").click(function(e){
          return false;
        });

        //promise
        function animateBox(){
          var promise = $.Deferred();
          $(".box").animate({left:"500px"},5000,function(){
            promise.resolve();
          });
          $(".box1").animate({left:"500px"},5000,function(){
            promise.resolve();
          });
          $(".box2").animate({left:"500px"},5000,function(){
            promise.resolve();
          });

          return promise.promise();
        }

        animateBox().done(function(){
          execute();
        });

        var execute = function()
        {
          alert("promise executed successfully.");
        }


     });
    	
    	
    </script>
  </head>
  <body style="background-image:url('/images/Elegant_Background-7.jpg');background-size:cover">
  	<div id="prop">
      <button class="btn btn-large btn-primary" id="test">Test</button>
      <a href="https://api.jquery.com/category/ajax/global-ajax-event-handlers/">testanchor </a>
    </div>
    <div class="box"></div>
    <div class="box1"></div>
    <div class="box2"></div>
  </body>
</html>
